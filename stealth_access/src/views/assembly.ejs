<%- include('base_top'); -%>
  <div class='appbody'>
    <h2>Assembly</h2>
    <table>
      <tr>
        <th>Device ID</th>
        <th>Board Serial</th>
        <th>Board Firmware</th>
      </tr>
      <% db.forEach(function(item){ %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.board %></td>
        <td><%= item.firmware %></td>
      </tr>
      <% }) %>
    </table>
  </div>
<%- include('base_bottom'); -%>