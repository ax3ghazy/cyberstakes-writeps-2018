<%- include('base_top'); -%>
  <div class='appbody'>
    <h2>Shipping Manifest</h2>
    <table>
      <tr>
        <th>Device ID</th>
        <th>Shipped Date</th>
        <th>Status</th>
      </tr>
      <% db.forEach(function(item){ %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.shippedDate %></td>
        <td><%= item.status %></td>
      </tr>
      <% }) %>
    </table>
  </div>
<%- include('base_bottom'); -%>