<%- include('base_top'); -%>
  <div class='appbody'>
    <h2>Quality Control Checks</h2>
    <table>
      <tr>
        <th>Device ID</th>
        <th>CPU Serial</th>
        <th>QA Test 1</th>
        <th>QA Test 2</th>
      </tr>
      <% db.forEach(function(item){ %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.cpu %></td>
        <td><%= item.test1 %></td>
        <td><%= item.test2 %></td>
      </tr>
      <% }) %>
    </table>
  </div>
<%- include('base_bottom'); -%>